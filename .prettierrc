{
  "useTabs": false,
  "tabWidth": 2,
  "bracketSpacing": true,
  "trailingComma": "es5",
  "arrowParens": "always",
  "bracketSameLine": true,
  "semi": true,
  "singleQuote": false,
  "quoteProps": "as-needed",
  "printWidth": 90,
  "overrides": [
    {
      "files": "*.html",
      "options": {
        "printWidth": 120
      }
    }
  ]
}
